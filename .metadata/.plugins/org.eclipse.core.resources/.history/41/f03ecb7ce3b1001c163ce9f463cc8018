package com.pichincha.fcpp.gestiondecuentes.dto;


import com.pichincha.fcpp.gestiondecuentes.entity.AccountEntity;
import com.pichincha.fcpp.gestiondecuentes.entity.MovementsEntity;

public class MovementsDto {
	
	private Integer movementId;

	private AccountEntity sourceAccountDetalles;

	private AccountEntity destinationAccountDetalles;
	
	private String movementDescription;

	private Double movementAmount;
	
	public MovementsEntity getAsTo() {
		return new MovementsEntity(
				this.movementId,
				this.sourceAccountDetalles, 
				this.destinationAccountDetalles, 
				this.movementDescription, 
				this.movementAmount 
				);
	}

}
